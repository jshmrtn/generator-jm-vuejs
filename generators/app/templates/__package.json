{
  "name": "<%= props.appName %>",
  "version": "0.0.0",
  "description": "<%= props.appDescription %>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "~ <%= props.nodeVersion %>",
    "npm": "~ <%= props.npmVersion %>"
  },
  "bin": {
    "setup-client-config": "./bin/setup-client-config"
  },
  "scripts": {
    "postinstall": "./bin/setup-client-config",
    "test": "npm run lint",
    "lint": "npm run lint:js && npm run lint:html",
    "lint:js": "./node_modules/.bin/eslint --ext .js,.vue src config build",
    "lint:html": "./node_modules/.bin/htmlhint src",
    <%_ if (props.bundlerType === 'webpack') { _%>
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    <%_ } _%>
    "clean": "rm -rf ./dist"
  },
  "browserslist": [
    "> 1%",
    "last 5 versions",
    "not ie < 10"
  ],
  "dependencies": {
    <%_ if (props.vuejsComponents.indexOf("routing") >= 0) { _%>
    "vue-router": "^2.5.3",
    <%_ } _%>
    <%_ if (props.vuejsComponents.indexOf("stateManagement") >= 0) { _%>
    "vuex": "^2.3.1",
    "vuex-persistedstate": "^2.0.0",
    <%_ } _%>
    <%_ if (props.vuejsComponents.indexOf("translations") >= 0) { _%>
    "vue-i18n": "^7.0.5",
    <%_ } _%>
    <%_ if (props.vuejsComponents.indexOf("httpClient") >= 0) { _%>
    "vue-resource": "^1.3.1",
    <%_ } _%>
    <%_ if (props.vuejsComponents.indexOf("graphqlClient") >= 0) { _%>
    "vue-apollo": "^2.1.0-beta.21",
    "apollo-client": "^1.0.1",
    <%_ } _%>
    <%_ if (props.vuejsComponents.indexOf("dependencyInjection") >= 0) { _%>
    "vue-inject": "^1.0.0",
    <%_ } _%>
    "vue": "^2.4.2",
    "reset-css": "^2.2.0",
    "modernizr": "^3.5.0"
  },
  "devDependencies": {
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint": "^4.3.0",
    "htmlhint": "^0.9.13",
    <%_ if (props.bundlerType === 'webpack') { _%>
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint": "^4.3.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "htmlhint": "^0.9.13",
    "http-proxy-middleware": "^0.17.4",
    "inject-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "less-loader": "^4.0.3",
    "less": "^2.7.2",
    "load-script": "^1.0.0",
    "modernizr-loader": "^1.0.1",
    "node-sass": "^4.5.2",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "ora": "^1.3.0",
    "postcss-loader": "^2.0.5",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.5",
    "sass": "^1.0.0-beta.2",
    "semver": "^5.3.0",
    "serviceworker-webpack-plugin": "^0.2.1",
    "shelljs": "^0.7.6",
    "style-loader": "^0.18.1",
    "svg-sprite-loader": "^3.0.8",
    "url-loader": "^0.5.8",
    "vue-hot-reload-loader": "^0.1.3",
    "vue-html-loader": "^1.2.4",
    "vue-template-compiler": "^2.4.2",
    "web-app-browserconfig-loader": "^0.0.0",
    "web-app-manifest-loader": "^0.1.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0",
    "webpack": "^3.4.1",
    <%_ if (props.browserconfig) { _%>
    "web-app-browserconfig-loader": "^0.0.0",
    <%_ } _%>
    <%_ if (props.manifest) { _%>
    "web-app-manifest-loader": "^0.1.1",
    <%_ } _%>
    <%_ } _%>
    "pre-commit": "^1.2.2"
  }
}
