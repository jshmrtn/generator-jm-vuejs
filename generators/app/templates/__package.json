{
  "name": "<%= props.appName %>",
  "version": "0.0.0",
  "description": "<%= props.appDescription %>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "~<%= props.nodeVersion %>"
  },
  "bin": {
    "quin-dev-server": "./bin/dev-server",
    "quin-setup-client-config": "./bin/setup-client-config"
  },
  "scripts": {
    "postinstall": "./bin/setup-client-config",
    "test": "npm run test:lint:js && npm run test:lint:html",
    "test:lint:js": "./node_modules/.bin/eslint .",
    "test:lint:html": "./node_modules/.bin/htmlhint .",
    <%_ if (props.bundlerType === 'webpack') { _%>
    "start": "npm run webpack:server",
    "watch": "npm run webpack:watch",
    "compile": "npm run build",
    "build": "npm run build:prod",
    "build:dev": "npm run webpack:dev",
    "build:prod": "npm run webpack:prod",
    "webpack:dev": "./node_modules/.bin/webpack --config webpack/dev.js --bail",
    "webpack:prod": "./node_modules/.bin/webpack --config webpack/prod.js --bail",
    "webpack:watch": "./node_modules/.bin/webpack --watch --config webpack/dev.js",
    "webpack:server": "OUTPUT_PATH='http://localhost:3000/' ./node_modules/.bin/webpack-dashboard -- ./bin/dev-server",
    <%_ } _%>
    "clean": "rm -rf ./dist"
  },
  "browserslist": [
    "> 1%",
    "last 5 versions",
    "not ie < 10"
  ],
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "autoprefixer": "^7.1.1",
    "modernizr": "^3.5.0",
    "modernizr-loader": "^1.0.1",
    "reset-css": "^2.2.0",
    <%_ if (props.bundlerType === 'webpack') { _%>
    "file-loader": "^0.11.1",
    "inject-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "load-script": "^1.0.0",
    "raw-loader": "^0.5.1",
    "extract-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "node-sass": "^4.5.2",
    "sass": "^1.0.0-beta.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "css-loader": "^0.28.1",
    "postcss-loader": "^2.0.5",
    "svg-sprite-loader": "^3.0.8",
    "url-loader": "^0.5.8",
    "serviceworker-webpack-plugin": "^0.2.1",
    "vue-html-loader": "^1.2.4",
    "vue-template-compiler": "^2.4.2",
    "vue-template-loader": "^0.3.1",
    "webpack": "^3.4.1",
    <%_ if (props.browserconfig) { _%>
    "web-app-browserconfig-loader": "^0.0.0",
    <%_ } _%>
    <%_ if (props.manifest) { _%>
    "web-app-manifest-loader": "^0.1.1",
    <%_ } _%>
    <%_ } _%>
    <%_ if (props.vuejsComponents.indexOf("routing") >= 0) { _%>
    "vue-router": "^2.5.3",
    <%_ } _%>
    <%_ if (props.vuejsComponents.indexOf("stateManagement") >= 0) { _%>
    "vuex": "^2.3.1",
    "vuex-persistedstate": "^2.0.0",
    <%_ } _%>
    <%_ if (props.vuejsComponents.indexOf("translations") >= 0) { _%>
    "vue-i18n": "^7.0.5",
    <%_ } _%>
    <%_ if (props.vuejsComponents.indexOf("httpClient") >= 0) { _%>
    "vue-resource": "^1.3.1",
    <%_ } _%>
    <%_ if (props.vuejsComponents.indexOf("graphqlClient") >= 0) { _%>
    "vue-apollo": "^2.1.0-beta.21",
    "apollo-client": "^1.0.1",
    <%_ } _%>
    <%_ if (props.vuejsComponents.indexOf("dependencyInjection") >= 0) { _%>
    "vue-inject": "^1.0.0",
    <%_ } _%>
    "vue": "^2.4.2"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "htmlhint": "^0.9.13",
    <%_ if (props.bundlerType === 'webpack') { _%>
    "browser-sync": "^2.18.11",
    "browser-sync-webpack-plugin": "^1.1.4",
    "http-proxy-middleware": "^0.17.4",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "null-loader": "^0.1.1",
    <%_ } _%>
    "pre-commit": "^1.2.2"
  }
}
