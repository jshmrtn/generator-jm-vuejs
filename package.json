{
  "name": "@jshmrtn/generator-jm-vuejs",
  "version": "1.0.0-rc.1",
  "description": "Generator for generating vuejs applications.",
  "homepage": "https://github.com/jshmrtn/generator-jm-vuejs",
  "author": {
    "name": "Jeremy Zahner",
    "email": "zahner@joshmartin.ch",
    "url": "https://joshmartin.ch"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "engine": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "generator",
    "vuejs",
    "spa",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^4.12.0",
    "eslint-config-xo-space": "^0.17.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "nsp": "^3.1.0",
    "package-json-validator": "^0.6.2",
    "semver-regex": "^1.0.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "babel-eslint": "^8.0.2",
    "chalk": "^2.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-vue": "^4.0.0-beta.0",
    "lodash": "^4.17.4",
    "snyk": "^1.106.1",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "__tests__/helpers/"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/*.js?(x)"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/generators"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "mapCoverage": true,
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "lcov",
      "html",
      "text"
    ]
  },
  "scripts": {
    "prepare": "npm run snyk-protect",
    "pretest": "eslint . --fix",
    "test": "snyk test; jest",
    "snyk-protect": "snyk protect"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "jest": true,
      "node": true
    }
  },
  "repository": "jshmrtn/generator-jm-vuejs",
  "license": "MIT",
  "snyk": true
}
